{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --force",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/trpc/(.*)", "destination": "/api/index.js" },
    { "source": "/api/oauth/(.*)", "destination": "/api/index.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
