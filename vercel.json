{
  "version": 2,
  "buildCommand": "npm install --force && npm run build",
  "installCommand": "npm install --force",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/trpc/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/oauth/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "vercel-node@latest"
    }
  }
}
